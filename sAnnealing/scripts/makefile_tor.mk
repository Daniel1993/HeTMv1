tor.max_cost : deps_tor_max_cost
	rm tor.max_cost ; \
	j=0 ; \
	for i in 32 64 128 256 512 1024; do \
	echo -ne "$$(($$j - 1))\t" >> tor.max_cost ; \
	cat tor_$${i}.data.max_cost >> tor.max_cost ; \
	echo -ne "\n$${j}\t" >> tor.max_cost ; j=$$(($$j + 1)) ; \
	cat tor_$${i}.data.max_cost >> tor.max_cost ; \
	echo "" >> tor.max_cost ; \
	done

